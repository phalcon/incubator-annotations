{
    "name": "phalcon/incubator-annotations",
    "description": "Phalcon Incubator Annotations",
    "keywords": [
        "framework",
        "phalcon",
        "incubator",
        "annotations"
    ],
    "homepage": "https://phalcon.io",
    "license": "BSD-3-Clause",
    "authors": [{
            "name": "Phalcon Team",
            "email": "team@phalcon.io",
            "homepage": "https://phalcon.io/en/team"
        },
        {
            "name": "Contributors",
            "homepage": "https://github.com/phalcon/incubator-annotations/graphs/contributors"
        }
    ],
    "support": {
        "issues": "https://github.com/phalcon/incubator/issues",
        "source": "https://github.com/phalcon/incubator-annotations",
        "forum": "https://forum.phalcon.io"
    },
    "require": {
        "php": ">=7.4",
        "ext-phalcon": "^5.0.0beta"
    },
    "require-dev": {
        "phalcon/ide-stubs": "^4.1",
        "vimeo/psalm": "^4.1",
        "squizlabs/php_codesniffer": "3.5.1",
        "codeception/codeception": "^4.1",
        "codeception/module-asserts": "^1.0.0",
        "phpstan/phpstan": "^0.12.18",
        "vlucas/phpdotenv": "^4.1",
        "phpunit/php-token-stream": "3.1.2",
        "webmozart/glob": "4.4.0"
    },
    "autoload": {
        "psr-4": {
            "Phalcon\\Incubator\\Annotations\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Phalcon\\Incubator\\Annotations\\Tests\\Unit\\": "tests/unit/",
            "Phalcon\\Incubator\\Annotations\\Tests\\Functional\\": "tests/functional/",
            "Phalcon\\Incubator\\Annotations\\Tests\\Fixtures\\": "tests/_data/fixtures/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "archive": {
        "exclude": [
            "/.github",
            "/tests",
            ".codacy.yml",
            ".codecov.yml",
            "CHANGELOG.md",
            "codeception.yml",
            "phpcs.xml.dist",
            "phpstan.neon.dist",
            "phpunit.xml.dist",
            "psalm.xml.dist"
        ]
    },
    "config": {
        "platform": {
            "php": "7.4"
        }
    }
}